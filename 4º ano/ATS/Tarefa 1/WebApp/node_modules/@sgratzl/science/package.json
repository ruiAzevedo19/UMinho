{
  "name": "@sgratzl/science",
  "version": "2.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "description": "Scientific and statistical computing in JavaScript.",
  "keywords": [
    "science",
    "statistics",
    "mathematics"
  ],
  "homepage": "https://github.com/sgratzl/science.js",
  "author": {
    "name": "Jason Davies",
    "url": "http://www.jasondavies.com/"
  },
  "contributors": [
    {
      "name": "Samuel Gratzl",
      "email": "sam@sgratzl.com",
      "url": "https://www.sgratzl.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/sgratzl/science.js.git"
  },
  "main": "build/science.js",
  "unpkg": "build/science.min.js",
  "module": "build/science.mjs",
  "files": [
    "build",
    "src/**/*.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "vows": "^0.8.0",
    "release-it": "^9.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.68.0",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-watch": "^4.3.1",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "clean": "rimraf build",
    "watch": "rollup -c -w -i src/index.js",
    "pretest": "npm run build:dev",
    "test": "vows",
    "test:watch": "vows --watch",
    "build:dev": "rollup -c -i src/index.js",
    "build:prod": "npm run build:dev && uglifyjs build/science.js -c -m -o build/science.min.js",
    "prebuild": "npm run clean && npm test",
    "build": "npm run build:prod",
    "preversion": "npm run test",
    "prepare": "npm run build:dev",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "release:pre": "release-it --preRelease=alpha --npm.tag=next"
  }
}
